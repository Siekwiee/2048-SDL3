{
  "editor.fontFamily": "'Iosevka', 'Iosevka Term', Menlo, Monaco, 'Courier New', monospace",
  "editor.fontLigatures": true,
  "terminal.integrated.fontFamily": "'Iosevka Term', 'Iosevka Nerd Font Mono', monospace",
  "debug.console.fontFamily": "'Iosevka Term', monospace",
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "scope": ["entity.name.type", "storage.type"],
        "settings": {
          "foreground": "#2eb63ee0"
        }
      },
      {
        "scope": ["entity.name.function"],
        "settings": {
          "foreground": "#fff156"
        }
      },
      {
        "scope": ["keyword"],
        "settings": {
          "foreground": "#a031aae0"
        }
      },
      {
        "scope": ["variable"],
        "settings": {
          "foreground": "#965bfd"
        }
      },
      {
        "scope": ["variable.name"],
        "settings": {
          "foreground": "#00c3ff"
        }
      }
    ]
  }
}
